  .zoomOut {
    transform: scale(0.3);
  }
  .hide {
    width: 0px;
    height: 0px;
  }
  .anyImage {
    width: 60%;
    height: 80%;
  }
  .anyImage:hover {
    transform: scale(1.2);
    z-index: 5 !important;
  }
  .imagePos1 {
    position: absolute;
    left: 90%;
    right: -100%;
    top:-80%;
    height: 100vh;
  }
  .imagePos2 {
    position: absolute;
    right: 100%;
    left: -96%;
    top:-80%;

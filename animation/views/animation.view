    height: 100vh;
    right:-105%;
  }
  .blur {
    background-color: rgb(128, 132, 136);

    /* Add the blur effect */
    filter: blur(8px);
    -webkit-filter: blur(8px);

    /* Full height */
    height: 100%;

    /* Center and scale the image nicely */
    background-position: center;
    background-repeat: no-repeat;
    background-size: cover;
    opacity: 0.7;
  }
  .justBlur{
    filter: blur(8px);
    -webkit-filter: blur(8px);
  }
</style>

<script>
    $("#zoomOut").click(function(){
      $("#mainDiv").toggleClass("zoomOut");
      $("#mainDiv").toggleClass('oh');
      $("#background").toggleClass('blur');
      
      move(1, [1,2], 100);
      move(2, [3,4], 200);
      move(3, [5,6], 300);
      move(4, [7,8], 400);
      move(5, [9,10], 500);
      move(6, [11,12], 600);
      move(7, [13,14], 700);
      move(8, [15,16], 800);
  });
  function move(parentNb, imageArray, time){
    setTimeout(function(){ 
          $("#parent" + parentNb).toggleClass("imagePos" + parentNb).toggleClass("cc").toggleClass("hide");
          $("#image" + imageArray[0]).toggleClass("cc");
          $("#image" + imageArray[1]).toggleClass("cc");
      }, time);
  }
  

$(".anyImage").click(function(){
  $("#carousel").toggleClass('h');
  $("#redDiv").toggleClass('justBlur');
  style = this.currentStyle || window.getComputedStyle(this, false),
  bi = style.backgroundImage.slice(4, -1).replace(/"/g, "");
  $('#backgroundImage').css("background-image", "url('"+bi+"')");
});
$("#closeButton").click(function(){
  $("#carousel").toggleClass('h');
  $("#redDiv").toggleClass('justBlur');
});
